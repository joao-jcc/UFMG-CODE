#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55dac99994b0 .scope module, "test" "test" 2 4;
 .timescale -9 -9;
v0x55dac99c1060_0 .var "D", 0 0;
v0x55dac99c1100_0 .net "Q", 0 0, v0x55dac9975ba0_0;  1 drivers
v0x55dac99c11d0_0 .var "clk", 0 0;
v0x55dac99c12d0_0 .var "reset", 0 0;
S_0x55dac99adb20 .scope module, "DFF" "Dflipflop" 2 11, 3 1 0, S_0x55dac99994b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "D";
    .port_info 3 /OUTPUT 1 "Q";
v0x55dac99757b0_0 .net "D", 0 0, v0x55dac99c1060_0;  1 drivers
v0x55dac9975ba0_0 .var "Q", 0 0;
v0x55dac99c0c20_0 .net "clk", 0 0, v0x55dac99c11d0_0;  1 drivers
v0x55dac99c0cf0_0 .net "reset", 0 0, v0x55dac99c12d0_0;  1 drivers
E_0x55dac9974810 .event posedge, v0x55dac99c0c20_0;
S_0x55dac99c0e60 .scope task, "display" "display" 2 49, 2 49 0, S_0x55dac99994b0;
 .timescale -9 -9;
TD_test.display ;
    %delay 1, 0;
    %vpi_call 2 50 "$display", "D:%0h, Q:%0h", v0x55dac99c1060_0, v0x55dac99c1100_0 {0 0 0};
    %end;
    .scope S_0x55dac99adb20;
T_1 ;
    %wait E_0x55dac9974810;
    %load/vec4 v0x55dac99c0cf0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55dac9975ba0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x55dac99757b0_0;
    %assign/vec4 v0x55dac9975ba0_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55dac99994b0;
T_2 ;
    %vpi_call 2 15 "$dumpfile", "dump.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars", 32'sb00000000000000000000000000000001 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55dac99c11d0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55dac99c12d0_0, 0, 1;
    %pushi/vec4 1, 1, 1;
    %store/vec4 v0x55dac99c1060_0, 0, 1;
    %fork TD_test.display, S_0x55dac99c0e60;
    %join;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55dac99c1060_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55dac99c12d0_0, 0, 1;
    %fork TD_test.display, S_0x55dac99c0e60;
    %join;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55dac99c11d0_0, 0, 1;
    %fork TD_test.display, S_0x55dac99c0e60;
    %join;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55dac99c11d0_0, 0, 1;
    %fork TD_test.display, S_0x55dac99c0e60;
    %join;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55dac99c11d0_0, 0, 1;
    %fork TD_test.display, S_0x55dac99c0e60;
    %join;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55dac99c11d0_0, 0, 1;
    %fork TD_test.display, S_0x55dac99c0e60;
    %join;
    %delay 5, 0;
    %vpi_call 2 46 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbench.v";
    "design.v";
