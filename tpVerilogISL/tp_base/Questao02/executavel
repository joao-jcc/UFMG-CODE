v0x56519a32e610_0 .var "clk", 0 0;
v0x56519a32e700_0 .net "digito_chave", 0 0, v0x56519a32deb0_0;  1 drivers
v0x56519a32e7d0_0 .var "mensagem_cifrada", 311 0;
v0x56519a32e870_0 .var "mensagem_original", 311 0;
S_0x56519a30e1a0 .scope task, "cifragem" "cifragem" 2 55, 2 55 0, S_0x56519a30e010;
 .timescale 0 0;
v0x56519a30e330_0 .var "msg", 311 0;
TD_test.cifragem ;
    %pushi/vec4 311, 0, 32;
    %store/vec4 v0x56519a32e470_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x56519a32e470_0;
    %cmpi/s 0, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz T_0.1, 5;
    %delay 20, 0;
    %load/vec4 v0x56519a30e330_0;
    %load/vec4 v0x56519a32e470_0;
    %part/s 1;
    %load/vec4 v0x56519a32e700_0;
    %xor;
    %ix/getv/s 4, v0x56519a32e470_0;
    %store/vec4 v0x56519a32e7d0_0, 4, 1;
    %load/vec4 v0x56519a32e470_0;
    %subi 1, 0, 32;
    %store/vec4 v0x56519a32e470_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
S_0x56519a32ca50 .scope task, "decifragem" "decifragem" 2 65, 2 65 0, S_0x56519a30e010;
 .timescale 0 0;
TD_test.decifragem ;
    %pushi/vec4 311, 0, 32;
    %store/vec4 v0x56519a32e470_0, 0, 32;
T_1.2 ;
    %load/vec4 v0x56519a32e470_0;
    %cmpi/s 0, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz T_1.3, 5;
    %delay 20, 0;
    %load/vec4 v0x56519a32e7d0_0;
    %load/vec4 v0x56519a32e470_0;
    %part/s 1;
    %load/vec4 v0x56519a32e700_0;
    %xor;
    %ix/getv/s 4, v0x56519a32e470_0;
    %store/vec4 v0x56519a32e870_0, 4, 1;
    %load/vec4 v0x56519a32e470_0;
    %subi 1, 0, 32;
    %store/vec4 v0x56519a32e470_0, 0, 32;
    %jmp T_1.2;
T_1.3 ;
    %end;
S_0x56519a32cc50 .scope task, "manipula_chave" "manipula_chave" 2 44, 2 44 0, S_0x56519a30e010;
 .timescale 0 0;
v0x56519a32ce30_0 .var "seed", 7 0;
TD_test.manipula_chave ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56519a32e310_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56519a32e3d0_0, 0, 1;
    %load/vec4 v0x56519a32ce30_0;
    %store/vec4 v0x56519a32e540_0, 0, 8;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56519a32e310_0, 0, 1;
    %end;
S_0x56519a32cef0 .scope module, "otp" "reg_streamcipher" 2 13, 3 1 0, S_0x56519a30e010;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "S1";
    .port_info 3 /INPUT 1 "S2";
    .port_info 4 /INPUT 8 "entrada";
    .port_info 5 /OUTPUT 1 "saida_e";
    .port_info 6 /OUTPUT 1 "saida_d";
    .port_info 7 /OUTPUT 8 "saida";
P_0x56519a32d0d0 .param/l "BIT_MAX" 1 3 13, +C4<00000000000000000000000000000111>;
P_0x56519a32d110 .param/l "BIT_MIN" 1 3 13, +C4<00000000000000000000000000000000>;
P_0x56519a32d150 .param/l "REG_MAX" 1 3 13, C4<11111111>;
L_0x56519a32eb50 .functor XOR 1, L_0x56519a32e930, L_0x56519a32ea30, C4<0>, C4<0>;
L_0x56519a32edd0 .functor XOR 1, L_0x56519a32ec60, L_0x56519a32ed00, C4<0>, C4<0>;
v0x56519a32d4a0_0 .net "S1", 0 0, v0x56519a32e310_0;  1 drivers
v0x56519a32d580_0 .net "S2", 0 0, v0x56519a32e3d0_0;  1 drivers
v0x56519a32d640_0 .net *"_ivl_1", 0 0, L_0x56519a32e930;  1 drivers
v0x56519a32d700_0 .net *"_ivl_3", 0 0, L_0x56519a32ea30;  1 drivers
v0x56519a32d7e0_0 .net *"_ivl_7", 0 0, L_0x56519a32ec60;  1 drivers
v0x56519a32d910_0 .net *"_ivl_9", 0 0, L_0x56519a32ed00;  1 drivers
v0x56519a32d9f0_0 .net "clk", 0 0, v0x56519a32e610_0;  1 drivers
v0x56519a32dab0_0 .net "entrada", 7 0, v0x56519a32e540_0;  1 drivers
v0x56519a32db90_0 .net "entrada_d", 0 0, L_0x56519a32edd0;  1 drivers
v0x56519a32dc50_0 .net "entrada_e", 0 0, L_0x56519a32eb50;  1 drivers
o0x7f61a0056258 .functor BUFZ 1, C4<z>; HiZ drive
v0x56519a32dd10_0 .net "reset", 0 0, o0x7f61a0056258;  0 drivers
v0x56519a32ddd0_0 .var "saida", 7 0;
v0x56519a32deb0_0 .var "saida_d", 0 0;
v0x56519a32df70_0 .var "saida_e", 0 0;
E_0x56519a2d0480 .event posedge, v0x56519a32d9f0_0;
L_0x56519a32e930 .part v0x56519a32ddd0_0, 0, 1;
L_0x56519a32ea30 .part v0x56519a32ddd0_0, 1, 1;
L_0x56519a32ec60 .part v0x56519a32ddd0_0, 7, 1;
L_0x56519a32ed00 .part v0x56519a32ddd0_0, 6, 1;
S_0x56519a32e130 .scope task, "resultados" "resultados" 2 74, 2 74 0, S_0x56519a30e010;
 .timescale 0 0;
TD_test.resultados ;
    %vpi_call 2 76 "$display", "\012Mensagem cifrada: %s", v0x56519a32e7d0_0 {0 0 0};
    %vpi_call 2 77 "$display", "Mensagem original: %s", v0x56519a32e870_0 {0 0 0};
    %end;
    .scope S_0x56519a32cef0;
T_4 ;
    %wait E_0x56519a2d0480;
    %load/vec4 v0x56519a32dd10_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.0, 8;
    %pushi/vec4 255, 0, 8;
    %assign/vec4 v0x56519a32ddd0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x56519a32df70_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x56519a32deb0_0, 0;
    %jmp T_4.1;
T_4.0 ;
    %load/vec4 v0x56519a32d4a0_0;
    %load/vec4 v0x56519a32d580_0;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.2, 8;
    %load/vec4 v0x56519a32dab0_0;
    %parti/s 1, 7, 4;
    %assign/vec4 v0x56519a32df70_0, 0;
    %load/vec4 v0x56519a32dab0_0;
    %parti/s 1, 0, 2;
    %assign/vec4 v0x56519a32deb0_0, 0;
    %load/vec4 v0x56519a32dab0_0;
    %assign/vec4 v0x56519a32ddd0_0, 0;
    %jmp T_4.3;
T_4.2 ;
    %load/vec4 v0x56519a32d4a0_0;
    %load/vec4 v0x56519a32d580_0;
    %nor/r;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.4, 8;
    %load/vec4 v0x56519a32ddd0_0;
    %parti/s 1, 7, 4;
    %assign/vec4 v0x56519a32df70_0, 0;
    %load/vec4 v0x56519a32ddd0_0;
    %parti/s 7, 0, 2;
    %load/vec4 v0x56519a32db90_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x56519a32ddd0_0, 0;
    %jmp T_4.5;
T_4.4 ;
    %load/vec4 v0x56519a32d4a0_0;
    %nor/r;
    %load/vec4 v0x56519a32d580_0;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.6, 8;
    %load/vec4 v0x56519a32ddd0_0;
    %parti/s 1, 0, 2;
    %assign/vec4 v0x56519a32deb0_0, 0;
    %load/vec4 v0x56519a32dc50_0;
    %load/vec4 v0x56519a32ddd0_0;
    %parti/s 7, 1, 2;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x56519a32ddd0_0, 0;
T_4.6 ;
T_4.5 ;
T_4.3 ;
T_4.1 ;
    %jmp T_4;
    .thread T_4;
    .scope S_0x56519a30e010;
T_5 ;
    %delay 10, 0;
    %load/vec4 v0x56519a32e610_0;
    %inv;
    %store/vec4 v0x56519a32e610_0, 0, 1;
    %jmp T_5;
    .thread T_5;
    .scope S_0x56519a30e010;
T_6 ;
    %vpi_call 2 22 "$dumpfile", "dump.vcd" {0 0 0};
    %vpi_call 2 23 "$dumpvars" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56519a32e610_0, 0, 1;
    %pushi/vec4 240, 0, 8;
    %store/vec4 v0x56519a32ce30_0, 0, 8;
    %fork TD_test.manipula_chave, S_0x56519a32cc50;
    %join;
    %pushi/vec4 2831869672, 0, 33;
    %concati/vec4 3393251556, 0, 32;
    %concati/vec4 3267674840, 0, 32;
    %concati/vec4 3504226528, 0, 32;
    %concati/vec4 3837978834, 0, 32;
    %concati/vec4 3336454374, 0, 32;
    %concati/vec4 3538348234, 0, 32;
    %concati/vec4 3670206016, 0, 32;
    %concati/vec4 3638480594, 0, 32;
    %concati/vec4 6516595, 0, 23;
    %store/vec4 v0x56519a30e330_0, 0, 312;
    %fork TD_test.cifragem, S_0x56519a30e1a0;
    %join;
    %pushi/vec4 240, 0, 8;
    %store/vec4 v0x56519a32ce30_0, 0, 8;
    %fork TD_test.manipula_chave, S_0x56519a32cc50;
