S_0x56519a30e010 .scope module, "test" "test" 2 1;
 .timescale 0 0;
v0x56519a32e310_0 .var "S1", 0 0;
v0x56519a32e3d0_0 .var "S2", 0 0;
v0x56519a32e470_0 .var/i "aux", 31 0;
